{
  "name": "dev20k-cli",
  "version": "0.4.0",
  "description": "Pro CLI commands that make your pocket filled with dollars, because you will become so good & efficient.",
  "preferGlobal": true,
  "directories": {
    "bin": "./bin",
    "lib": "./lib"
  },
  "bin": {
    "xd": "./dist/xd.js"
  },
  "files": [
    "./dist/xd.js",
    "./package.json",
    "./README.md"
  ],
  "main": "./lib/index.js",
  "scripts": {
    "lint": "eslint lib/** --fix",
    "start": "rollup -c --watch",
    "build": "rollup -c"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint",
      "post-update": "npm i"
    }
  },
  "engines": {
    "node": ">=12.16.1",
    "npm": ">=6.13.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebdybowski/dev20k-cli.git"
  },
  "keywords": [
    "dev20k",
    "dev20k-cli",
    "dybowski",
    "sebdybowski",
    "cli",
    "dev",
    "20k",
    "npm",
    "bash",
    "git"
  ],
  "author": "Seb Dybowski <dybowski@int.pl>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sebdybowski/dev20k-cli/issues"
  },
  "homepage": "https://github.com/sebdybowski/dev20k-cli#readme",
  "dependencies": {
    "chalk": "^5.0.1",
    "commander": "^10.0.0",
    "lodash.noop": "^3.0.1",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "eslint": "^8.20.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "rollup": "^2.77.2",
    "rollup-plugin-terser": "^7.0.2"
  }
}
